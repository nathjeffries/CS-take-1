package com.PointClass;

public class PointClass {
    private double x = 0;
    private double y = 0;

    public PointClass(double x, double y){

    }

    public double getX() {
        return this.x;
    }

    public double getY() {
        return this.y;
    }

    public void FlipVertical() {
      this.x = -this.x;

    }

    public void FlipHorizontal() {
      this.y = -this.y;

    }

    public void Translate(double xChange, double yChange){
      this.x = this.x + xChange;
      this.y = this.y + yChange;


    }
}